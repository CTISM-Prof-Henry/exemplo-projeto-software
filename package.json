{
    "type": "module",
    "devDependencies": {
        "@babel/core": "^7.28.4",
        "@babel/preset-env": "^7.28.3",
        "@babel/register": "^7.28.3",
        "@eslint/js": "^9.37.0",
        "eslint": "^9.37.0",
        "globals": "^16.4.0",
        "nyc": "^17.1.0",
        "qunit": "^2.24.1"
    },
    "scripts": {
        "test": "nyc qunit \"website/static/js/test/**/*.js\""
    },
    "nyc": {
        "require": ["@babel/register"],
        "babel": true,
        "reporter": ["text", "html"],
        "include": ["website/static/js/src/**/*.js"],
        "exclude": ["website/static/js/test/**/*.js"],
        "all": true,
        "extension": [".js"]
    }
}
